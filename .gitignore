# Angular build output directories
/dist/
/out-tsc/

# Node.js dependencies
/node_modules/

# Environment files (only if you want to keep sensitive info local)
**/src/environments/*.local.ts

# Compiled TypeScript files
*.js
*.js.map

# IDE-specific files (e.g., Visual Studio Code, IntelliJ IDEA, etc.)
.vscode/
.idea/
*.sublime-workspace

# Log files
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Temporary files
/tmp
/temp

# Angular CLI specific files
.angular
.angular/cache

# MacOS specific files
.DS_Store

# Optional: Exclude local environments for Angular CLI
# If you use different environment files for production and development, include the following:
src/environments/environment.local.ts
src/environments/environment.prod.ts

# Ignore test coverage reports
/coverage/

# Ignore package lock files if you want to keep dependencies flexible
package-lock.json
yarn.lock

# Ignore generated files for Windows, Linux, and Mac
Thumbs.db
*.DS_Store
.vercel
